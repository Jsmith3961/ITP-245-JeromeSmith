@using ITP_Fall2024_JeromeSmith4.Models;
@model About

@{
    ViewBag.Title = "About";
}
<main aria-labelledby="title">
    <h1>@Model.Name</h1>
    <p>@Model.Email</p>
    <p>@Model.Hometown</p>
    <p>@Model.Major</p>
    <p>@Model.Education</p>
    <table> <thead> <tr> <th>Experience</th> </tr> </thead> <tbody> @foreach (var name in Model.Experience)
    {<tr> <td>@name</td> </tr>} </tbody> </table>

</main>