@using ITP245_Agile_Model
@Model Dictionary<string, List<IAgile></IAgile>
    @{
        ViewBag.Title = "Home Page";
    }

    <main>
        <section class="row" aria-labelledby="aspnetTitle">
            <h1 id="title">Agile/Sports Project Management</h1>
            <p class="lead">ASP.NET is a free web framework for building great Web sites and Web applications using HTML, CSS and JavaScript.</p>
            <p><a href="https://asp.net" class="btn btn-primary btn-md">Learn more &raquo;</a></p>
        </section>

        <div class="row">
            @foreach (var agile in Model)
            {

            
            <section class="col-md-4" aria-labelledby="gettingStartedTitle">
                <h2> @agile.Key</h2>
                <p>
                    @foreach (var project in agile.value)
                    {
                        <div class="row">
                            <div class="col-lg-6">
                                <ul>
                                    <li>
                                        @project.Name
                                    </li>
                                </ul>
                            </div>
                        </div>
                    }
                </p>
            </section>
            }
        </div>
    </main>
