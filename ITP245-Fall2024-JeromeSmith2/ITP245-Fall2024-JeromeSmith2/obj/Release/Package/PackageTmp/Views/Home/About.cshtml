@using ITP245_Fall2024_JeromeSmith2.Models;
@model About

@{
    ViewBag.Title = "About";
}
<main aria-labelledby="title">
    <h1>@Model.Name</h1>
    <p>@Model.Email</p>
    <p>@Model.Hometown</p>
    <p>@Model.Major</p>
    <p>@Model.Education</p>

    <table>
        <tr>
            <th>@Model.Experience</th>
        </tr>
        @foreach (var name in Model.Experience)
        {
            <tr>
                <td>@name</td>
            </tr>

        }
    </table>

</main>